<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>AssetWorks</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>AssetWorks Reimagined</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>18</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSSupportsLiveActivities</key>
	<true/>
	<key>NSSupportsLiveActivitiesFrequentUpdates</key>
	<true/>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>AssetWorks needs access to your photo library to allow you to upload images for your investment widgets and portfolio visualizations.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>AssetWorks needs permission to save generated charts and reports to your photo library.</string>
	<key>NSCameraUsageDescription</key>
	<string>AssetWorks needs camera access to capture documents and receipts for your investment records.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>AssetWorks needs microphone access for voice notes on your investment strategies.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>AssetWorks uses Face ID to secure access to your financial data.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>AssetWorks uses your location to provide region-specific market data and comply with financial regulations.</string>
	<key>NSUserTrackingUsageDescription</key>
	<string>AssetWorks uses this information to provide personalized investment insights and improve the app experience.</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>NSUserActivityTypes</key>
	<array>
		<string>com.assetworks.viewPortfolio</string>
		<string>com.assetworks.viewStock</string>
		<string>com.assetworks.trade</string>
		<string>com.assetworks.viewChart</string>
		<string>com.assetworks.viewWatchlist</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>processing</string>
		<string>remote-notification</string>
	</array>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>ai.assetworks.mobile.refresh</string>
		<string>ai.assetworks.mobile.widgetupdate</string>
		<string>ai.assetworks.mobile.datasync</string>
		<string>ai.assetworks.mobile.cleanup</string>
	</array>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
	</array>
	<key>UIApplicationSupportsMultipleScenes</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
</dict>
</plist>
